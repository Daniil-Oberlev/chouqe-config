{
  "name": "Daniil_Oberlev-config",
  "version": "1.0.0",
  "description": "Core configuration for JS/TS projects",
  "license": "MIT",
  "author": "Daniil-Oberlev",
  "type": "module",
  "files": [
    ".github/",
    ".czrc",
    ".dockerignore",
    ".editorconfig",
    ".env.example",
    ".gitattributes",
    ".gitignore",
    ".npmrc",
    ".nvmrc",
    ".prettierignore",
    ".prettierrc.js",
    "Dockerfile",
    "eslint.config.js",
    "lefthook.yml",
    "tsconfig.app.json",
    "tsconfig.json",
    "tsconfig.node.json",
    "README.md"
  ],
  "keywords": [
    "config",
    "eslint",
    "prettier",
    "typescript"
  ],
  "config": {
    "commitizen": {
      "path": "./.czrc"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commit": "cz",
    "release": "standard-version --skip.commit --skip.tag"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "https://github.com/Daniil-Oberlev/chouqe-config#readme",
  "dependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@eslint/js": "^9.22.0",
    "@evilmartians/lefthook": "^1.11.3",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.11",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2"
  }
}
